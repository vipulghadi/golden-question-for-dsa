

int missing=1,count=0;
        for(int i=0;i<arr.size();)
        {
            if(arr[i]!=missing)
            {
                missing++;
                count++;
                if(count==k)return missing -1;
            }
            else{
                missing++;
                i++;
            }
        }
        return(k-count+missing-1);
